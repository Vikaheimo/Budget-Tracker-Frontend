{
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    
    "ignorePatterns": ["node_modules"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"]
    }
}
